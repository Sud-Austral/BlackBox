                    @*<li class="breadcrumb-item active" aria-current="page">@ViewBag.Elemento.titulo</li>*@
                </ol>
            </nav>
        </div>


        <!--descargar pdf iframe-->
        @*<button style="cursor: -webkit-grab; cursor: grab; margin-bottom:10px;">Descargar PDF</button>*@
        <script type="text/javascript">
            $("#verPdf").click(function () {
                var myIframe = document.getElementById("inlineFrameExample").contentWindow;
                myIframe.focus();
                myIframe.print();
                return false;
            });
        </script>
        @*seccion iframe grafico*@
        <section class="contenedor-main row mr-md-2">
            <main class="col-md-12" style="border:1px solid #e6e6e6; border-radius:10px; border-top:none; height:800px !important;">
