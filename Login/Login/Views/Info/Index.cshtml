                    </div>
                    <div class="col">
                        <p class="mt-3" style="color:#fff;">@ViewBag.Elemento.titulo</p>
                    </div>
                </div>
                <!--Iframe de grafico-->
                <iframe id="inlineFrameExample"
                        style=" width:100%;height:95%;"
                        scrolling="no"
                        src="/info/dashboard?id=@ViewBag.Elemento.id"></iframe>

                <div class="row no-gutters" style="color:blue; font-size:10px;">

                    <div class="col-2 mr-3">
                        <button type="button" class="btn btn-primary" id="boton" style="margin-bottom:20px;">Información del Gráfico</button>
                    </div>

                    <div class="col-md-1 col-2 text-center mt-2 mb-5 ml-md-1 mr-md-1" style="border-right:1px solid #d9d9d9;">
                        <a href="javascript:void(0);">
                            <span id="verPdf" style="font-size:14px;">Ver en Pdf</span>
                        </a>
                        <script type="text/javascript">
                            $("#verPdf").click(function () {
                                var myIframe = document.getElementById("inlineFrameExample").contentWindow;
                                myIframe.focus();
                                myIframe.print();
                                return false;
                            });
                        </script>
                    </div>
                    <div class="col">
                        <div class="row d-flex">
                            <div class="col-5 mb-1 ml-auto p-1">
                                <img class="img-fluid " src="~/Content/img/logosDataIntelligence_creadopor.png" alt="Logo transparente" />
                            </div>
                        </div>
                    </div>
                </div>
                <div>
                    <script>

                    </script>
                </div>
            </main>
