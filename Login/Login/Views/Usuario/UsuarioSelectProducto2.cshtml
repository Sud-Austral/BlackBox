<div class="row mt-5">

    <div class="col-md-8">
        <h3 class="text-dark mb-0">Informes Interactivos</h3>
        <hr style="border:0.5px solid #000000" />
    </div>

</div>

<div class="row">
    <div class="col">
        @*<P><strong> Se muestran <span>@ViewBag.Resultado.Count </span>resultados de Informes Interactivos</strong></P>*@
    </div>
</div>
<div class="row responsive" style="overflow:scroll;height:300px;overflow-x:hidden; width:67%;">

    @foreach (var item in ViewBag.Resultado2)

    {
        <div class="col-md-11">
            <div class="row" style="display:flex !important; cursor: pointer;">
                
                    <div class="col-md-2">
                        <img class="img-fluid" src="~/Content/image_UsuarioIndex/icono_informeinteractivo_AZUL.png" />
                    </div>
                    <div class="col-md-9 text-dark font-weight-bold mb-0" style="color:#518bb3 !important;">
                        <a onclick="mostrador('#quince'); fnClick2('@item.id');">
                            <h6 class="mt-3">@item.titulo</h6>
                            @*@item.NOMBRE <br />
                            @item.FECHA_CREADO*@
                        </a>
                    </div>
                
            </div>
        </div>

    }
</div>
